start:
	npm start

local-backend:
	npm run local-backend

lint:
	npm run lint

build:
	npm run build

deploy-dev-from-local:
	npm run deploy-dev-from-local

deploy-prod-from-local:
	npm run deploy-prod-from-local

update:
	npm run fetch-data

build-size:
	npm run size

analyze:
	npm run analyze

install:
	npm i
