{
  "name": "babajka-frontend",
  "version": "0.0.1",
  "description": "Next.js & React app",
  "scripts": {
    "dev": "next -p 3001",
    "build": "next build",
    "prod": "next build && next start -p 3000",
    "deploy": "now",
    "precommit": "lint-staged",
    "lint": "npm run prettify && eslint '{!(.next*|*static*|node_modules*), }/**/*.js' --fix",
    "prettier": "prettier --config .prettierrc --write",
    "prettify": "npm run prettier '{!(.next*|*static*), }/**/*.{js,md}' '*.md'",
    "watch": "onchange '**/*.{js,md}' -- npm run prettier {{changed}}",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "sh copy-static.sh",
    "update-styles": "npm i --save babajka-markup && sh copy-static.sh",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook",
    "storybook:deploy": "build-storybook && gh-pages -d storybook-static"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "babajka-markup": "^0.1.2",
    "next": "^3.2.2",
    "next-redux-wrapper": "^1.3.4",
    "now": "^7.4.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-form": "^2.12.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "^4.4.2",
    "redux-thunk": "^2.2.0",
    "type-to-reducer": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babajka/babajka-frontend.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/babajka/babajka-frontend/issues"
  },
  "homepage": "https://github.com/babajka/babajka-frontend#readme",
  "main": "index.js",
  "devDependencies": {
    "@storybook/addon-info": "^3.2.13",
    "@storybook/addon-knobs": "^3.2.13",
    "@storybook/react": "^3.2.13",
    "babel-eslint": "^8.0.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "classnames": "^2.2.5",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "onchange": "^3.2.1",
    "prettier": "^1.8.2",
    "raw-loader": "^0.5.1"
  },
  "moduleRoots": [
    "."
  ]
}
