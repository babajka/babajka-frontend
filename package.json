{
  "name": "babajka-frontend",
  "version": "0.0.1",
  "description": "Next.js & React app",
  "scripts": {
    "start": "npm run dev",
    "start:prod": "npm run build && cross-env NODE_ENV=production node server.js",
    "dev": "npm run reset-cache && node server.js --port=3000",
    "local-styles": "cross-env DEBUG_STYLES=true npm run dev",
    "local-backend": "npm run dev -- --backend_url=http://localhost:8080",
    "local-styles-backend": "cross-env DEBUG_STYLES=true npm run local-backend",
    "build": "cross-env NODE_ENV=production next build",
    "precommit": "lint-staged",
    "lint": "npm run prettify && eslint '{!(.next*|*static*|node_modules*), }/**/*.js' --fix",
    "prettier": "prettier --config .prettierrc --write",
    "prettify": "npm run prettier '{!(.next*|*static*), }/**/*.{js,md}' '*.md'",
    "prettify:watch": "onchange '**/*.{js,md}' -- npm run prettier {{changed}}",
    "update-styles": "npm i --save babajka-markup@latest && npm run copy-static",
    "update-dict": "node bin/fetchDict.js",
    "update-team": "node bin/fetchTeam.js",
    "copy-static": "sh bin/copy-static.sh",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook",
    "storybook:deploy": "build-storybook && gh-pages -d storybook-static",
    "reset-cache": "rm -rf node_modules/.cache && rm -rf .next/",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:client": "BUNDLE_ANALYZE=browser npm run build",
    "postinstall": "npm run copy-static && npm run update-dict && npm run update-team",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run prettier",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@storybook/addon-actions": "^3.2.19",
    "@storybook/addon-info": "^3.2.19",
    "@storybook/addon-knobs": "^3.2.19",
    "@storybook/react": "^3.2.19",
    "Dante2": "git+https://github.com/babajka/dante2.git",
    "babajka-markup": "^0.8.18",
    "classnames": "^2.2.5",
    "command-line-args": "^5.0.2",
    "cookie-parser": "^1.4.3",
    "downshift": "^1.31.2",
    "express": "^4.16.2",
    "google-spreadsheet": "^2.0.5",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "next": "^6.1.1",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.4.1",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.4.0",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.4.0",
    "react-form": "^2.15.2",
    "react-ga": "^2.4.1",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "redraft": "^0.10.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "type-to-reducer": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babajka/babajka-frontend.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/babajka/babajka-frontend/issues"
  },
  "homepage": "https://github.com/babajka/babajka-frontend#readme",
  "main": "index.js",
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.54",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.1",
    "@zeit/next-bundle-analyzer": "^0.1.1",
    "babel-eslint": "^8.0.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "onchange": "^3.2.1",
    "prettier": "^1.8.2",
    "semantic-release": "^15.9.5",
    "travis-deploy-once": "^5.0.2",
    "webpack": "^3.10.0"
  },
  "moduleRoots": [
    "."
  ]
}
