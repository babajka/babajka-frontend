{
  "name": "babajka-frontend",
  "version": "2.1.1",
  "description": "Next.js & React app",
  "scripts": {
    "start": "npm run dev",
    "start:prod": "npm run build && cross-env NODE_ENV=production node server.js",
    "start:prod:remote": "node server.js",
    "dev": "npm run reset-cache && node server.js --port=3000",
    "local-backend": "npm run dev -- --backend_url=http://localhost:8080",
    "deploy-dev-from-local": "bash bin/deploy/deploy-from-local.sh dev",
    "deploy-prod-from-local": "bash bin/deploy/deploy-from-local.sh prod",
    "build": "cross-env NODE_ENV=production next build",
    "lint": "npm run prettify && eslint '{!(.next*|*static*|node_modules*), }/**/*.js' --fix && npm run stylelint",
    "stylelint": "stylelint '{!(.next*|*static*|node_modules*), }/**/*.scss' --fix",
    "prettier": "prettier --write",
    "prettify": "npm run prettier '{!(.next*|*static*|lib), }/**/*.{js,md,scss}' '*.md' '*.js'",
    "update-dict": "bash bin/fetch.sh dict",
    "update-team": "bash bin/fetch.sh team",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook",
    "storybook:deploy": "build-storybook && gh-pages -d storybook-static",
    "reset-cache": "rm -rf node_modules/.cache && rm -rf .next/",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:client": "BUNDLE_ANALYZE=browser npm run build",
    "fetch-data": "npm run update-dict && npm run update-team",
    "postinstall": "git submodule update --init && npm run fetch-data",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "size-build": "npm run reset-cache && npm run build",
    "size-limit": "size-limit",
    "size": "npm run size-build && size-limit",
    "gds": "github-deploy-status -u babajka -p babajka-frontend"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@storybook/addon-actions": "^5.0.7",
    "@storybook/addon-knobs": "^5.0.7",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/react": "^5.0.7",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "bem-css-modules": "^1.4.2",
    "classnames": "^2.2.6",
    "command-line-args": "^4.0.7",
    "cookie-parser": "^1.4.4",
    "date-fns": "^2.13.0",
    "env-ci": "^3.2.0",
    "express": "^4.17.1",
    "formik": "^1.5.2",
    "generate-json-webpack-plugin": "^0.3.1",
    "google-spreadsheet": "^3.0.5",
    "http-proxy-middleware": "^0.20.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.15",
    "next": "^9.3.3",
    "next-compose-plugins": "^2.2.0",
    "next-cookies": "^1.1.3",
    "next-redux-wrapper": "^3.0.0",
    "next-routes": "^1.4.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "qs": "^6.6.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-ga": "^2.5.7",
    "react-id-swiper": "^3.0.0",
    "react-redux": "^6.0.0",
    "react-scroll-up": "^1.3.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.2",
    "swiper": "^5.3.8",
    "type-to-reducer": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babajka/babajka-frontend.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/babajka/babajka-frontend/issues"
  },
  "homepage": "https://github.com/babajka/babajka-frontend#readme",
  "main": "index.js",
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/npm": "^5.1.13",
    "@size-limit/preset-app": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "gh-pages": "^2.0.1",
    "github-deploy-status": "^1.4.1",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^2.0.1",
    "semantic-release": "^15.13.19",
    "size-limit": "^4.5.0",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.16.0",
    "travis-deploy-once": "^5.0.11"
  },
  "moduleRoots": [
    "."
  ]
}
