{
  "name": "babajka-frontend",
  "version": "1.0.1",
  "description": "Next.js & React app",
  "scripts": {
    "start": "npm run dev",
    "start:prod": "npm run build && cross-env NODE_ENV=production node server.js",
    "start:prod:remote": "node server.js",
    "dev": "npm run reset-cache && node server.js --port=3000",
    "local-styles": "cross-env DEBUG_STYLES=true npm run dev",
    "local-backend": "npm run dev -- --backend_url=http://localhost:8080",
    "local-styles-backend": "cross-env DEBUG_STYLES=true npm run local-backend",
    "build": "cross-env NODE_ENV=production next build",
    "lint": "npm run prettify && eslint '{!(.next*|*static*|node_modules*), }/**/*.js' --fix",
    "prettier": "prettier --config .prettierrc --write",
    "prettify": "npm run prettier '{!(.next*|*static*), }/**/*.{js,md}' '*.md'",
    "update-styles": "npm i --save babajka-markup@latest && npm run copy-static",
    "update-dict": "node bin/fetchDict.js",
    "update-team": "node bin/fetchTeam.js",
    "copy-static": "sh bin/copy-static.sh",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook",
    "storybook:deploy": "build-storybook && gh-pages -d storybook-static",
    "reset-cache": "rm -rf node_modules/.cache && rm -rf .next/",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:client": "BUNDLE_ANALYZE=browser npm run build",
    "fetch-data": "npm run copy-static && npm run update-dict && npm run update-team",
    "postinstall": "patch-package && npm run fetch-data",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "bundlesize": "bundlesize",
    "size": "npm run reset-cache && npm run build && npm run bundlesize"
  },
  "dependencies": {
    "@storybook/addon-actions": "^4.0.0",
    "@storybook/addon-info": "^4.0.0",
    "@storybook/addon-knobs": "^4.0.1",
    "@storybook/addon-links": "^4.0.0",
    "@storybook/react": "^4.0.0",
    "Dante2": "git+https://github.com/babajka/dante2.git",
    "babajka-markup": "^1.0.12",
    "classnames": "^2.2.5",
    "command-line-args": "^5.0.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.4",
    "downshift": "^1.31.2",
    "express": "^4.16.2",
    "formik": "^1.3.1",
    "google-spreadsheet": "^2.0.6",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "next": "^7.0.1",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.4.1",
    "patch-package": "^5.1.1",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.4.0",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.4.0",
    "react-form": "^2.15.2",
    "react-ga": "^2.4.1",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "redraft": "^0.10.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "type-to-reducer": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babajka/babajka-frontend.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/babajka/babajka-frontend/issues"
  },
  "homepage": "https://github.com/babajka/babajka-frontend#readme",
  "main": "index.js",
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/exec": "^3.1.3",
    "@semantic-release/git": "^7.0.4",
    "@semantic-release/npm": "^5.0.4",
    "@zeit/next-bundle-analyzer": "^0.1.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "bundlesize": "^0.17.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.2",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9",
    "webpack": "^4.20.2"
  },
  "moduleRoots": [
    "."
  ],
  "bundlesize": [
    {
      "path": ".next/static/**/commons.*.js",
      "maxSize": "150 kB"
    },
    {
      "path": ".next/static/**/editArticle.js",
      "maxSize": "200 kB"
    },
    {
      "path": ".next/static/**/about.js",
      "maxSize": "5 kB"
    },
    {
      "path": ".next/static/**/article.js",
      "maxSize": "15 kB"
    },
    {
      "path": ".next/static/**/index.js",
      "maxSize": "10 kB"
    },
    {
      "path": ".next/static/**/login.js",
      "maxSize": "15 kB"
    },
    {
      "path": ".next/static/**/_error.js",
      "maxSize": "5 kB"
    }
  ]
}
