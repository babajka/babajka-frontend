{
  "name": "babajka-frontend",
  "version": "0.0.1",
  "description": "Next.js & React app",
  "scripts": {
    "start": "npm run dev",
    "dev": "export PORT=3000 && node server.js",
    "local-backend": "export BABAJKA_BACKEND_URL=http://localhost:8080 && npm run dev",
    "build": "next build",
    "start-prod": "npm run build && NODE_ENV=production node server.js",
    "precommit": "lint-staged",
    "lint": "npm run prettify && eslint '{!(.next*|*static*|node_modules*), }/**/*.js' --fix",
    "prettier": "prettier --config .prettierrc --write",
    "prettify": "npm run prettier '{!(.next*|*static*), }/**/*.{js,md}' '*.md'",
    "prettify:watch": "onchange '**/*.{js,md}' -- npm run prettier {{changed}}",
    "update-styles": "npm i --save babajka-markup@latest && npm run copy-static",
    "update-dict": "node bin/fetchDict.js",
    "copy-static": "sh bin/copy-static.sh",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook",
    "storybook:deploy": "build-storybook && gh-pages -d storybook-static",
    "postinstall": "npm run copy-static && npm run update-dict"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run prettier",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@storybook/addon-actions": "^3.2.19",
    "@storybook/addon-info": "^3.2.19",
    "@storybook/addon-knobs": "^3.2.19",
    "@storybook/react": "^3.2.19",
    "babajka-markup": "^0.7.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "downshift": "^1.31.2",
    "express": "^4.16.2",
    "google-spreadsheet": "^2.0.4",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "next": "^5.1.0",
    "next-redux-wrapper": "^1.3.5",
    "next-routes": "^1.4.1",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-form": "^2.15.2",
    "react-no-ssr": "^1.1.0",
    "react-quill": "^1.2.3",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "type-to-reducer": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/babajka/babajka-frontend.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/babajka/babajka-frontend/issues"
  },
  "homepage": "https://github.com/babajka/babajka-frontend#readme",
  "main": "index.js",
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-define": "^1.3.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "onchange": "^3.2.1",
    "prettier": "^1.8.2",
    "raw-loader": "^0.5.1"
  },
  "moduleRoots": [
    "."
  ]
}
